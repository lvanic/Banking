<UserControl x:Class="Banking.Views.TransferView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Banking.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TextBox HorizontalAlignment="Left" Margin="194,58,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="239" x:Name="Bill"/>
        <Button Content="{DynamicResource m_Send}" HorizontalAlignment="Left" Margin="194,104,0,0" VerticalAlignment="Top" Width="239" Click="Button_Click"/>
        <Label Content="{DynamicResource m_EnterBill}" HorizontalAlignment="Left" Margin="194,32,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Margin="194,81,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="239" x:Name="MoneyAmount"/>


    </Grid>
</UserControl>
